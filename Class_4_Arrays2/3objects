// create
let emp = {
    'eid':101,
    'ename':'Rahul',
    'esal':45000.45
}
// read
console.log(emp)
// Read object properties
console.log(emp.eid)
console.log(emp.ename)
console.log(emp.esal)
console.log(emp.loc) // undefined property
// update
emp.ename = 'Rahul Gandhi'
console.log(emp)
emp.email = 'rahul@gmail.com'
console.log(emp)
// 
delete emp.esal
console.log(emp)